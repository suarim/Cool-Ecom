{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch Program--email-service",
            "skipFiles": [
                "<node_internals>/**"
            ],
            "program": "${file}"
        },
        {
            "type": "node-terminal",
            "name": "Run Script: start-->email-service",
            "request": "launch",
            "command": "npm run start",
            "cwd": "${workspaceFolder}/email-service"
        },
        {
            "type": "node-terminal",
            "name": "Run Script: start",
            "request": "launch",
            "command": "npm run start",
            "cwd": "${workspaceFolder}/email-service"
        }
    ]
}